<%-include('header',data) %>
    <div id="content"><br>
        <%data=JSON.parse(data)%>
            <%for(const container of data.containers){%>
                <div>
                    <div class="jump" <%-container.extra%>> </div>
                    <br>
                    <div class="container <%-container.classes%>">
                        <h3>
                            <%-container.header%>
                        </h3>
                        <p>
                            <%-container.content%>
                        </p>
                    </div>
                </div>
                <%}%>
    </div>
    <%-include footer%>